{
    "name": "AppTv",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["declarativeContent", "storage", "tabs", "activeTab", "*://*/*"],
    "background": {
      "scripts": ["js/socket.io.js", "background.js"],
      "persistent": false
    },
    "icons": {
        "16": "iconos/ICONETE16x16.png",
        "32": "iconos/ICONETE32x32.png",
        "48": "iconos/ICONETE48x48.png",
        "128": "iconos/ICONETE128x128.png"
    },
    "browser_action": {
        "default_title": "",
        "default_icon": "iconos/ICONETE128x128.png"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self'; object-src 'self'; "
  }
